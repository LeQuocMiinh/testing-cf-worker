{
	"compilerOptions": {
	  "target": "ES2020",
	  "outDir": "./dist",
	  "rootDir": "./src",
	  "declaration": true,
	  "module": "CommonJS",
	  "baseUrl": "./",
	  "resolveJsonModule": true,
	  "esModuleInterop": true,
	  "noPropertyAccessFromIndexSignature": false,
	  "strict": true,
	  "plugins": [
		{
		  "transform": "typia/lib/transform"
		}
	  ],
	  "strictNullChecks": true,
	  "noImplicitAny": false,
	  "allowSyntheticDefaultImports": true
	},
	"include": [
	  "./src/**/*"
	],
	"exclude": [
	  "node_modules",
	  "./dist/**/*"
	]
  }